@ARTICLE{Althaus2009560,
author={Althaus, H.-J. and De Haan, P. and Scholz, R.W.},
title={Traffic noise in LCA: Part 1: State-of-science and requirement profile for consistent context-sensitive integration of traffic noise in LCA},
journal={International Journal of Life Cycle Assessment},
year={2009},
volume={14},

author={Mitropoulos, L. and Adamos, G. and Nathanail, E. and Yatskiv (Jackiva), I. and Kabashkin, I.},
title={Building on European scientific excellence to develop an educational program on intermodal connections for Latvia and the region},
journal={Transport},
year={2017},
volume={32},
number={1},
